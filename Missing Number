int missingNumber(int* a, int n){
    int i,d,sum1=0,sum2=0;
    for(i=0;i<=n;i++)
    {
        sum1=sum1+i;
    }
    for(i=0;i<n;i++)
    {
        sum2=sum2+a[i];
    }

    if(sum1==sum2)
    {
        return 0;
    }
    else
    {
        d=sum1-sum2;
       // printf("%d",d);
       return d;

    }

}
