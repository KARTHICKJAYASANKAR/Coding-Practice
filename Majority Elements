class Solution {
public:
    int majorityElement(vector<int>& nums) {
        unordered_map<int,int> m;
        int max,maj;
        max=0;
        for(auto x : nums)
        {
            m[x]++;
        }
        for(auto x : m)
        {
            if(x.second > max)
            {
                max=x.second;
                maj=x.first;
            }
        }
        return maj;
    }
};
